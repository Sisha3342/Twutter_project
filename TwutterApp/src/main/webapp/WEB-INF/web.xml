<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>PostAction</servlet-name>
    <servlet-class>twutter.PostActionServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CheckServlet</servlet-name>
    <servlet-class>twutter.CheckServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>GetNameServlet</servlet-name>
    <servlet-class>twutter.GetNameServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>PageServlet</servlet-name>
    <servlet-class>twutter.PageServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>StatusServlet</servlet-name>
    <servlet-class>twutter.StatusServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Test1Servlet</servlet-name>
    <servlet-class>twutter.Test1Servlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Test2Servlet</servlet-name>
    <servlet-class>twutter.Test2Servlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PostAction</servlet-name>
    <url-pattern>/tweets/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CheckServlet</servlet-name>
    <url-pattern>/check</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>GetNameServlet</servlet-name>
    <url-pattern>/get</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>PageServlet</servlet-name>
    <url-pattern>/page</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>StatusServlet</servlet-name>
    <url-pattern>/status</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Test1Servlet</servlet-name>
    <url-pattern>/test1</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Test2Servlet</servlet-name>
    <url-pattern>/test2</url-pattern>
  </servlet-mapping>

</web-app>
